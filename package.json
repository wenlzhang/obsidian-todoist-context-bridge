{
    "name": "obsidian-simple-todoist-sync",
    "version": "0.0.1",
    "description": "Sync Obsidian tasks with Todoist, with support for Advanced URI links",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json"
    },
    "keywords": [
        "obsidian",
        "todoist",
        "task-management",
        "sync"
    ],
    "author": "wenlzhang",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.17.3",
        "obsidian": "latest",
        "tslib": "2.4.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@doist/todoist-api-typescript": "^2.0.0",
        "moment": "^2.29.4"
    }
}
